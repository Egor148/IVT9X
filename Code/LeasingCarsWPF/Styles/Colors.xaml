<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="BackgroundColor">#4A5459</Color>
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{StaticResource BackgroundColor}"></SolidColorBrush>

    <Color x:Key="PrimaryColor">#99d1ff</Color>
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryColor}"></SolidColorBrush>

    <Color x:Key="PrimaryHighLightColor">#c7e6ff</Color>
    <SolidColorBrush x:Key="PrimaryHighLightColorBrush" Color="{StaticResource PrimaryHighLightColor}"></SolidColorBrush>








    <Color x:Key="BlueGray">#69797E</Color>
    <Color x:Key="CoolBlueBright">#0099BC</Color>
    <Color x:Key="CoolBlue">#2d7d9a</Color>
    <Color x:Key="GreyDark">#4a5459</Color>
    <Color x:Key="FluentWhite">#e9e8e8</Color>

    <SolidColorBrush x:Key="BlueGrayBrush" Color="{StaticResource BlueGray}"></SolidColorBrush>
    <SolidColorBrush x:Key="CoolBlueBrightBrush" Color="{StaticResource CoolBlueBright}"></SolidColorBrush>
    <SolidColorBrush x:Key="CoolBlueBrush" Color="{StaticResource CoolBlue}"></SolidColorBrush>
    <SolidColorBrush x:Key="GreyDarkBrush" Color="{StaticResource GreyDark}"></SolidColorBrush>
    <SolidColorBrush x:Key="FluentWhiteBrush" Color="{StaticResource FluentWhite}"></SolidColorBrush>

</ResourceDictionary>